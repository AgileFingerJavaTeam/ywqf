<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ywqf.dao.AuthRoleDao">
    <!--目的:为dao接口方法提供sql语句配置
    即针对dao接口中的方法编写我们的sql语句-->

	<!--查询导航栏信息  -->
    <select id="findAuthRole"  resultType="AuthRole">
        SELECT *
        FROM zx_auth_rule
        WHERE pid=#{id}
    </select>


</mapper>